<div class="dashboard-card">
    <div class="dashboard-card-content-wrapper font-size-14px" >
        <div>
            <p class="dashboard-card-header"><%= pageText.peopleYouMayKnow %></p>
            <small>.</small>
            <a class="refresh-link"><%= pageText.refresh %></a>
            <small>.</small>
            <a class="show-all-link"><%= pageText.showAll %></a>
            <div>
                <ul class="possible-friend-list">
                    <% for (var index = 0; index < possibleFriends.length; index++) { %>
                    <% if (index != possibleFriends.length - 1) { %> 
                    <li>
                      <div class="possible-friend">
                        <a class="possible-friend-picture" style="background-image: url(<%= possibleFriends[index].profilePicture %>);"></a>
                        <p class="possible-friend-text-wrapper"><a style="color: black;"><%= possibleFriends[index].userName %></a><i class="fa fa-times on-people-you-may-know" aria-hidden="true"></i></p>
                        <a class="add-friend-button"><i class="fa fa-plus" aria-hidden="true"></i><%= pageText.addFriend %></a>
                      </div>
                    </li>
                    <% } else { %>
                    <li>
                      <div class="possible-friend last-in-list" >
                        <a class="possible-friend-picture" style="background-image: url(<%= possibleFriends[index].profilePicture %>);"></a>
                        <p class="possible-friend-text-wrapper"><a style="color: black;"><%= possibleFriends[index].userName %></a><i class="fa fa-times on-people-you-may-know" aria-hidden="true"></i></p>
                        <a class="add-friend-button"><i class="fa fa-plus" aria-hidden="true"></i><%= pageText.addFriend %></a>
                      </div>
                    </li>
                    <% } %>
                    <% } %>
                </ul>
            </div>
        </div>
    </div>
</div>